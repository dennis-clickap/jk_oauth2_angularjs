{
  "name": "JK OAuth2-AngularJS",
  "version": "1.2",
  "description": "A demonstration of integrating OAuth2 from Moodle in a Google Chrome Extension; After get_Access_token",

  "manifest_version": 2,
  "homepage_url": "https://www.click-ap.com",
  "browser_action": {
    "default_title": "JK OAuth2-AngularJS-Test",
    "default_popup": "app/views/main.html"
  },
  "key": "",
  "background": {
    "scripts": [
      "app/scripts/background.js"
    ]
  },
  "oauth2": {
    "client_id": "13044057060-vfnppe053mphvn77m2qr7j8sts6jt7j2.apps.googleusercontent.com",
    "scopes": [ "openid", "profile" ]
  },
  "permissions": [
    "https://www.moodle.com.tw/*", 
    "https://www.googleapis.com/*", 
    "identity", "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "200": "icons/icon200.png"
 }
}